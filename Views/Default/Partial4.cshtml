@using TravelTripProject.Models.Siniflar
@model List<Blog>
<div class="padding1">
    <h4>Favori Yerlerimiz</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
        <a href="/Blog/BlogDetay/@x.ID">
            <div class="bot-img">
                <img src="@x.BlogImage" alt="" />
            </div>
            <div class="bot-desc">
                <p>@x.Aciklama.Substring(0, 75)...</p>
            </div>
            <div class="clear"></div>
        </a>
        }
    </div>
    @Html.Action("Partial5", "Default")
</div>